#!/home/shub/devel/personal/mozdownload/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'mozinfo','console_scripts','mozinfo'
__requires__ = 'mozinfo'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mozinfo', 'console_scripts', 'mozinfo')()
    )
